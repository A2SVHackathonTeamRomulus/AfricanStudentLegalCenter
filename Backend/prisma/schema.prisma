// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:../databases/dev.db"
}

model User {
  id              Int    @id @default(autoincrement())
  email String  @unique
  name  String?
  password String?
}

model Blog{
  id              Int    @id @default(autoincrement())
  title           String
  content         String
  publisher       String
}

model Contacts {
  id        Int     @id @default(autoincrement())
  country   String
  sector    String
  phone     String
}


